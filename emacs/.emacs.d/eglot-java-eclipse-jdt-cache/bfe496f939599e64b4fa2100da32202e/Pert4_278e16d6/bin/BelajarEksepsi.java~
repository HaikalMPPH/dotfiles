class BelajarEksepsi {
        public static void main(String[] args) {
                String nilai[] = new String[3];
               

                // try 1 //
                try {
                        nilai[0] = "0";
                        nilai[1] = "1";
                        nilai[2] = "dua";
                        nilai[3] = "tiga";
                } catch (ArrayIndexOutOfBoundsException ex) {
                        System.out.println("ini blok program catch karena try mengalami error");
                } finally {
                        System.out.println("kode program try terlewati");
                }
               

                // try 2 //
                try {
                        int hasil = Integer.parseInt(nilai[2]);
                } catch (NumberFormatException e) {
                        System.out.println("try kedua juga gagal.");
                } finally {
                        System.out.println("kode program try kedua juga dilewati");
                }


                // try 3 //
                try {
                        int a = Integer.parseInt(nilai[0]);
                        int b = Integer.parseInt(nilai[1]);

                        int hasil = b / a;
                } catch (ArithmeticException e) {
                        System.out.println("try ketiga gagal");
                } finally {
                        System.out.println("semua try gagal");
                }
        }
}
